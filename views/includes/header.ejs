										<option value="<%=data.id%>" <% if(user.branch.id === data.id) {%> selected <%} %> ><%=data.name%></option>	
									<%}) %>
								</select>
							</div>
							<div style="margin-top: 10px;">
								<label for="selectGodown" class="form-label required">Select Godown</label>
