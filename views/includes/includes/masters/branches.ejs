   function allowOnlyNumbers(event) {
  const inputValue = event.target.value;
  const regex = /^[0-9]*$/;

  if (!regex.test(inputValue)) {
    // If the input value contains non-numeric characters, remove them
    event.target.value = inputValue.replace(/[^0-9]/g, '');
  }
}

